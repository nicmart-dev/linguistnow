PORT=8080
BACKEND_URL=http://localhost:8080

# Google OAuth vars
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=${BACKEND_URL}

# Airtable vars
AIRTABLE_PERSONAL_ACCESS_TOKEN=your_personal_access_token_here
AIRTABLE_BASE_ID=your_airtable_base_id_here

# N8N Configuration
# For remote n8n (HTTPS): https://your-n8n-instance.com
# For local n8n: http://localhost:5678
N8N_BASE_URL=http://localhost:5678
# Optional: Custom webhook path (defaults to /webhook/calendar-check)
# N8N_WEBHOOK_PATH=/webhook/calendar-check
